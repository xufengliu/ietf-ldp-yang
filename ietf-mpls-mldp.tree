         |                 +--ro peer                  leafref
         |                 +--ro advertisement-type    ldp:advertised-received
         |                 +--ro label?                mpls:mpls-label
         |                 +--ro mbb-role?             enumeration
         |                 +--ro mofrr-role?           enumeration
