         +--rw config
         |  +--rw multicast-only-frr {mldp-mofrr}?
         |  |  +--rw prefix-list?   ldp:prefix-list-ref
         |  +--rw recursive-fec
         |     +--rw prefix-list?   ldp:prefix-list-ref
         +--ro state
         |  +--ro multicast-only-frr {mldp-mofrr}?
         |  |  +--ro prefix-list?   ldp:prefix-list-ref
         |  +--ro recursive-fec
         |  |  +--ro prefix-list?   ldp:prefix-list-ref
