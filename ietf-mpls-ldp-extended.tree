   +--rw address-families
      +--rw ipv4
      |  +--rw label-policy
      |     +--rw advertise
      |     |  +--rw prefix-list?   prefix-list-ref
      |     +--rw accept
      |        +--rw prefix-list?   prefix-list-ref
      +--rw ipv6
         +--rw label-policy
            +--rw advertise
            |  +--rw prefix-list?   prefix-list-ref
            +--rw accept
