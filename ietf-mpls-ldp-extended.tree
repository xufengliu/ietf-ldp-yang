   +--rw admin-down?            boolean
   +--rw label-policy
   |  +--rw advertise
   |  |  +--rw prefix-list?   prefix-list-ref
   |  +--rw accept
   |     +--rw prefix-list?   prefix-list-ref
   +--rw graceful-restart
   |  +--rw enable?           boolean
   |  +--rw reconnect-time?   uint16
   |  +--rw recovery-time?    uint16
   +--rw session-ka-holdtime?   uint16
   +--rw session-ka-interval?   uint16
